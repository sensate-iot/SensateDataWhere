{
  "name": "sensate-data-map",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot=false --disableHostCheck=true --base-href /apps/datawhere/ --port 4300",
    "build-dev": "ng build --base-href /apps/datawhere/",
    "build-staging": "ng build --configuration=staging --base-href /apps/datawhere/",
    "build": "ng build --aot --configuration=production --base-href /apps/datawhere/",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "moment": "^2.29.0",
    "leaflet": "^1.6.0",
    "ngx-cookie-service": "^3.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.11",
    "ts-node": "~8.3.0",
    "typescript": "3.9.7"
